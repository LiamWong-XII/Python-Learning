#print(resultado)
import math
def calculate (a, b, operación):
    if operación == "suma":
        return a + b
    elif operación == "resta":
        return a - b
    elif operación == "multiplicación":
        return a * b
    elif operación == "división":
        return a / b
    elif operación == "potencia":
        return a * a
    elif operación == "raíz cuadrada":
        return math.sqrt(a)
    else:
        return "Operación invalida"
    
print(calculate(14, 0, "potencia"))
print(calculate(5, 3, "multiplicación"))
print(calculate(196, 0, "raíz cuadrada"))

print ("hola mundo")